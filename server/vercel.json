{
  "version": 2,
  "builds": [
    { "src": "server/index.js", "use": "@vercel/node" },
    { "src": "client/package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "http://localhost:8888/(.*)", "dest": "server/index.js" },
    { "src": "/(.*)", "dest": "client/build/$1" }
  ]
}
